{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape"],
        "jsdoc-format": false,
        "no-console": false,
        "interface-name": false,
        "prefer-for-of": false,
        "max-line-length": [true, 200]
    },
    "rulesDirectory": []
}
